onProvider","TextInput","FileInput","RichTextInput","CvModels","EmploymentHistoryModel","EducationModels","SkillsModel","api","FromCv","constructor","value","text","cekTitle","cv","jobTitle","isActive","addEmployment","dataEmployment","listEmployment","push","removeEmployment","i","splice","addEducation","dataEducation","listEducation","removeEducation","openTextSkill","skill","displayTextSkill","addSkills","skills","listSkills","removeSkills","onSubmit","form","submitForm","then","response","data","code","alert","catch","err","components"],"sourceRoot":"","sources":["/Users/macbook/Documents/Project/vuejs/telkom-test/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/macbook/Documents/Project/vuejs/telkom-test/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/macbook/Documents/Project/vuejs/telkom-test/src/views/FormCv.vue?vue&type=script&lang=ts&"],"sourcesContent":["\nimport Vue from 'vue'\nimport { Component } from 'vue-property-decorator'\nimport { ValidationObserver, ValidationProvider } from 'vee-validate'\nimport TextInput from '../components/input/TextInput.vue'\nimport FileInput from '../components/input/FileInput.vue'\nimport RichTextInput from '../components/input/RichTextInput.vue'\nimport CvModels from '../models/CvModels'\nimport EmploymentHistoryModel from '../models/EmploymentHistoryModel'\nimport EducationModels from '../models/EducationModels'\nimport SkillsModel from '../models/SkillsModel'\nimport api from '../api/input'\n\n@Component({\n  components: {\n    TextInput,\n    FileInput,\n    RichTextInput,\n    ValidationObserver,\n    ValidationProvider\n  }\n})\nexport default class FromCv extends Vue {\n  isActive = false;\n  skill: string | null = null;\n  displayTextSkill = false\n  listJob = [\n    { value: null, text: 'Please select a job title' },\n    { value: 'SE', text: 'Software Engineer' },\n    { value: 'BA', text: 'Bussiness Analyst' },\n    { value: 'QA', text: 'Quality Assurance' }\n  ]\n\n  listEducation = [\n    { value: null, text: 'Please select a education' },\n    { value: 'S3', text: 'S3' },\n    { value: 'S2', text: 'S2' },\n    { value: 'S1', text: 'S1' },\n    { value: 'SMA', text: 'SMA' },\n    { value: 'SMP', text: 'SMP' },\n    { value: 'SD', text: 'SD' }\n  ]\n\n    cv: CvModels = new CvModels();\n\n    cekTitle () {\n      if (this.cv.jobTitle !== '') {\n        this.isActive = true\n      } else if (this.cv.jobTitle === '') {\n        this.isActive = false\n      }\n    }\n\n    addEmployment () {\n      const dataEmployment = new EmploymentHistoryModel()\n      this.cv.listEmployment.push(dataEmployment)\n    }\n\n    removeEmployment (i: number) {\n      this.cv.listEmployment.splice(i, 1)\n    }\n\n    addEducation () {\n      const dataEducation = new EducationModels()\n      this.cv.listEducation.push(dataEducation)\n    }\n\n    removeEducation (i: number) {\n      this.cv.listEducation.splice(i, 1)\n    }\n\n    openTextSkill () {\n      this.skill = null\n      this.displayTextSkill = true\n    }\n\n    addSkills () {\n      const skills = new SkillsModel()\n      skills.skill = this.skill\n      this.cv.listSkills.push(skills)\n      this.displayTextSkill = false\n    }\n\n    removeSkills (i: number) {\n      this.cv.listSkills.splice(i, 1)\n    }\n\n    onSubmit () {\n      api.form\n        .submitForm(this.cv)\n        .then((response) => {\n          if (response.data.code === 200) {\n            alert('success')\n          } else {\n            alert('error')\n          }\n        })\n        .catch((err: Error) => {\n          alert(err)\n        })\n    }\n}\n"]}]}